<!DOCTYPE html>
<html lang="en">
        
    <head>

        {{>head}}

    </head> 
    
    <body>

        <div class="container-fluid allHeight">
            
            {{>header}}

            <br><br>



            {{!-- TEMPLATE START --}}

            <div class="container-fluid">
                <div class="row">
                    
                    <div class="col-md-6 offset-md-3">
                        <div class="" id="myForm">
                            <h3 class="text-center rangeText">Select the level of compression</h3>
                            <input type="range" class="rangeSlider" min="5" max="100" value="50" step="5" id="myRange">
                            <h4 class="text-center rangeText" id="valueRange">50%</h4>
                            <input type="file" id="myFile" name="filename" class="myFileInput">
                            <p id="warningText" class="setInVisible">Select the correct file!</p>
                            <input type="button" class="btn btn-outline-primary myButton" value="Compress" id="submitBtn">
                        </div>
                    </div>
                    
                </div>
            </div>

            {{!-- TEMPLATE END --}}



        </div>

        {{>footer}}
        
        {{>scripts}}
        
    </body>

</html>